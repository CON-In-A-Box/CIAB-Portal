{
    "name": "con-in-a-box/ciab-signin",
    "description": "Convention Meetings, Registration and Volunteers management software package",
    "config": {
	"platform": {
	    "php": "7.2.7"
	}
    },
    "keywords": [
        "calendar",
        "scheduler",
        "events",
        "dates",
        "google",
        "services"
    ],
    "type": "project",
    "homepage": "https://github.com/CON-In-A-Box/CIAB-Signin",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Thomas Keeley"
        },
        {
            "name": "Aric Stewart",
            "email": "aricstewart@gmail.com"
        }
    ],
    "support": {
        "source": "https://github.com/CON-In-A-Box/CIAB-Signin"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.6",
        "google/apiclient": "^2.0",
        "bshaffer/oauth2-server-php": "^1.10",
        "vlucas/phpdotenv": "^3.6",
        "zendframework/zend-permissions-rbac": "^3.0",
        "sendgrid/sendgrid": "^7.3",
        "guzzlehttp/psr7": "^1.6",
        "http-interop/http-factory-guzzle": "^1.0",
        "slim/slim": "3.*",
        "chadicus/slim-oauth2": "^3.1",
        "slim/php-view": "^2.2",
        "scssphp/scssphp": "^1.2",
        "scssphp/server": "^1.0",
        "stripe/stripe-php": "^7.67",
        "atlas/query": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "api/src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    }
}
